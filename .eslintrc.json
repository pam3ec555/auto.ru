{
    "extends": "airbnb-base",
    "rules": {
        "indent": [2, 4],
        "quotes": [2, "backtick"],
        "no-console": "off",
        "max-len": [2, {
            "code": 120
        }],
        "comma-dangle": [0],
        "object-curly-spacing": 0,
        "arrow-parens": 0,
        "no-undef": 1,
        "arrow-body-style": 0,
        "no-underscore-dangle": 0,
        "no-await-in-loop": 0
    },
    "parserOptions": {
        "ecmaVersion": 8
    }
}
